<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myschool">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".parent.ViewStatus" />
        <activity android:name=".parent.MessegeParent" />
        <activity android:name=".teacher.MessageTeacher" />
        <activity android:name=".student.DetailHomework" />
        <activity android:name=".student.StudentUpload" />
        <activity android:name=".student.StudentDownload" />
        <activity android:name=".teacher.TeacherDownload" />
        <activity android:name=".teacher.TeacherUpload" />
        <activity android:name=".student.StudentHomeActivity" />
        <activity android:name=".parent.ParentHomeActivity" />
        <activity android:name=".parent.ParentRegisterActivity" />
        <activity android:name=".student.StudentRegisterActivity" />
        <activity android:name=".teacher.TeacherHome" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".teacher.RegisterTeacherActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>