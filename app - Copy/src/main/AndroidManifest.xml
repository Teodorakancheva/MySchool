<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myschool">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.myschool.student.StudentHomeActivity" />
        <activity android:name=".parent.ParentHomeActivity" />
        <activity android:name=".parent.ParentRegisterActivity" />
        <activity android:name=".student.StudentRegisterActivity" />
        <activity android:name=".teacher.TeacherHome" />
        <activity android:name=".teacher.search.SearshStudent" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".teacher.RegisterTeacherActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>